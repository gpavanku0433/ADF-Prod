{
	"name": "DS_Archieve",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_HRP_BLOB",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Rename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@{concat(substring(dataset().Rename,1,add(indexof(dataset().Rename,'.'),-1)),utcNow(),'.txt')}",
					"type": "Expression"
				},
				"container": "hrparchieve"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}